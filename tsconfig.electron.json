{
	"compilerOptions": {
		"incremental": true,
		"inlineSourceMap": true,
		"strict": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"target": "es5",
		"lib": [
			"dom",
			"es2016",
			"es2017"
		],
		"esModuleInterop": true,
	},
	"include": [
		"./app/main.ts",
		"./app/renderer.ts",
		"./app/*",
	],
	"exclude": [
		"./app/src/*"
	]
}
