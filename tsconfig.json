{
	"compilerOptions": {
		"sourceMap": true,
		"strict": true,
		"noUnusedLocals": true,
		"resolveJsonModule": true,
		"downlevelIteration": true,
		"module": "esnext",
		"moduleResolution": "node",
		"target": "es5",
		"lib": [
			"dom",
			"es2016",
			"es2017"
		],
		"esModuleInterop": true,
		"jsx": "react",
		"baseUrl": "app/src"
	},
	"include": [
		"./app/src/*"
	]
}
