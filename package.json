{
    "name": "typescript-react-starter-kit",
    "repository": "https://github.com/hyldmo/typescript-react-starter-kit",
    "version": "1.1.0",
    "scripts": {
        "start": "env-cmd ./.env electron app",
        "dev": "env-cmd ./.env webpack-dev-server --config webpack/webpack.development.config.ts --progress --colors --hot",
        "prod": "env-cmd ./.env webpack --config  webpack/webpack.production.config.ts --progress --colors",
        "test:unit": "jest --config jest.config.js",
        "test:cover": "jest --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
        "lint": "tslint {__tests__,app}/**/*.{ts,tsx} webpack.*.ts --exclude **/*.d.ts",
        "stylelint": "stylelint app/**/*.less --syntax less",
        "stats": "env-cmd ./.env webpack --config webpack.production.config.ts --profile --json > compilation-stats.json"
    },
    "keywords": [
        "Electron",
        "Typescript",
        "React",
        "Redux",
        "CSSNext"
    ],
    "license": "MIT",
    "dependencies": {
        "classnames": "^2.2.6",
        "electron": "^5.0.0",
        "history": "^4.6.3",
        "joi-browser": "^13.4.0",
        "normalize.css": "^8.0.0",
        "react": "~16.8.2",
        "react-dom": "~16.8.2",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "redux": "^4.0.0",
        "redux-saga": "^0.16.0",
        "serialport": "^7.1.5"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.6",
        "@types/fs-extra": "^5.0.4",
        "@types/history": "^4.6.0",
        "@types/jest": "^23.0.0",
        "@types/joi": "^14.0.0",
        "@types/mini-css-extract-plugin": "0.2.0",
        "@types/node": "*",
        "@types/pixelmatch": "^4.0.0",
        "@types/pngjs": "^3.3.2",
        "@types/react": "*",
        "@types/react-dom": "^16.8.2",
        "@types/react-redux": "^7.0.0",
        "@types/react-router": "^4.0.18",
        "@types/react-router-dom": "^4.2.2",
        "@types/react-router-redux": "^5.0.10",
        "@types/react-test-renderer": "^16.8.1",
        "@types/redux-actions": "^2.2.3",
        "@types/serialport": "^7.0.3",
        "@types/webpack": "*",
        "@types/webpack-dev-server": "^3.1.5",
        "@types/webpack-env": "^1.13.9",
        "bundle-loader": "^0.5.6",
        "coveralls": "^3.0.0",
        "css-loader": "^2.1.0",
        "env-cmd": "^8.0.2",
        "fs-extra": "^7.0.0",
        "jest": "^23.6.0",
        "less": "^3.9.0",
        "less-loader": "^5.0.0",
        "mini-css-extract-plugin": "^0.4.0",
        "pixelmatch": "^4.0.2",
        "postcss": "^7.0.0",
        "postcss-loader": "^3.0.0",
        "react-hot-loader": "^4.7.1",
        "react-test-renderer": "^16.8.2",
        "redbox-react": "^1.5.0",
        "style-loader": "^0.23.0",
        "stylelint": "^10.0.1",
        "stylelint-config-recommended": "^2.2.0",
        "ts-jest": "^23.10.4",
        "ts-loader": "^5.2.2",
        "ts-node": "^7.0.0",
        "tslint": "^5.6.0",
        "tslint-eslint-rules": "^5.1.0",
        "typescript": "~3.1.1",
        "wait-on": "^3.1.0",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.0"
    },
    "resolutions": {
        "@types/node": "^10.1.3",
        "@types/react": "16.8.4",
        "@types/webpack": "^4.4.29"
    }
}
