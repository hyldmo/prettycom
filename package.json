{
    "name": "prettycom",
    "repository": "https://github.com/hyldmo/typescript-react-starter-kit",
    "version": "1.1.0",
    "description": "A serial monitor written in electron.",
    "license": "MIT",
    "author": {
        "name": "Eivind Hyldmo"
    },
    "keywords": [
        "Electron",
        "Typescript",
        "React",
        "Redux",
        "CSSNext"
    ],
    "scripts": {
        "dev": "concurrently --kill-others-on-fail \"yarn electron:build\" \"yarn webpack:dev\" \"yarn webpack:wait\"",
        "build": "concurrently \"yarn lint\" \"yarn stylelint\" \"yarn electron:build\" \"yarn webpack:build\"",
        "dist": "electron-packager . --out Release --overwrite --ignore \"(^__*)\"",
        "webpack:dev": "env-cmd ./.env webpack-dev-server --config webpack/webpack.development.config.ts --progress --colors --hot",
        "webpack:wait": "wait-on http://localhost:1337 && yarn electron:dev",
        "webpack:build": "env-cmd ./.env webpack --config  webpack/webpack.production.config.ts --progress --colors",
        "electron:build": "env-cmd ./.env tsc --project tsconfig.electron.json",
        "electron:dev": "env-cmd ./.env electron app",
        "lint": "tslint {__tests__,app}/**/*.{ts,tsx} webpack.*.ts --exclude **/*.d.ts",
        "stylelint": "stylelint app/**/*.scss --syntax scss",
        "stats": "env-cmd ./.env webpack --config webpack.production.config.ts --profile --json > compilation-stats.json",
        "postinstall": "electron-rebuild"
    },
    "main": "./app/main.js",
    "build": {
        "buildDependenciesFromSource": false,
        "files": [
            "./app/**/*",
            "./app/main.js",
            "./app/index.html",
            "./app/renderer.js"
        ]
    },
    "dependencies": {
        "serialport": "^7.1.5",
        "ws": "^7.0.0"
    },
    "devDependencies": {
        "@types/classnames": "^2.2.6",
        "@types/fs-extra": "^5.0.4",
        "@types/history": "^4.6.0",
        "@types/html-webpack-plugin": "^3.2.0",
        "@types/mini-css-extract-plugin": "0.2.0",
        "@types/node": "*",
        "@types/pixelmatch": "^4.0.0",
        "@types/pngjs": "^3.3.2",
        "@types/react": "*",
        "@types/react-dom": "^16.8.2",
        "@types/react-redux": "^7.0.0",
        "@types/react-router": "^4.0.18",
        "@types/react-router-dom": "^4.2.2",
        "@types/react-router-redux": "^5.0.10",
        "@types/react-test-renderer": "^16.8.1",
        "@types/redux-actions": "^2.2.3",
        "@types/redux-logger": "^3.0.7",
        "@types/serialport": "^7.0.3",
        "@types/webpack": "*",
        "@types/webpack-dev-server": "^3.1.5",
        "@types/webpack-env": "^1.13.9",
        "@types/ws": "^6.0.1",
        "bulma": "^0.7.4",
        "classnames": "^2.2.6",
        "concurrently": "^4.1.0",
        "css-loader": "^2.1.0",
        "electron": "^5.0.1",
        "electron-packager": "^13.1.1",
        "electron-rebuild": "^1.8.4",
        "env-cmd": "^8.0.2",
        "fs-extra": "^7.0.0",
        "history": "^4.6.3",
        "html-webpack-harddisk-plugin": "^1.0.1",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.0",
        "node-sass": "^4.12.0",
        "normalize.css": "^8.0.0",
        "postcss": "^7.0.0",
        "postcss-loader": "^3.0.0",
        "react": "~16.8.2",
        "react-dom": "~16.8.2",
        "react-hot-loader": "^4.8.4",
        "react-redux": "^5.0.6",
        "react-router": "^4.2.0",
        "react-router-dom": "^4.2.2",
        "react-router-redux": "^5.0.0-alpha.6",
        "react-test-renderer": "^16.8.2",
        "redbox-react": "^1.5.0",
        "redux": "^4.0.0",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.0.2",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.0",
        "stylelint": "^10.0.1",
        "stylelint-config-recommended": "^2.2.0",
        "stylelint-config-recommended-scss": "^3.3.0",
        "stylelint-scss": "^3.6.1",
        "ts-loader": "^5.2.2",
        "ts-node": "^7.0.0",
        "tslint": "^5.6.0",
        "tslint-eslint-rules": "^5.1.0",
        "typescript": "~3.4.5",
        "wait-on": "^3.1.0",
        "webpack": "^4.29.5",
        "webpack-cli": "^3.2.3",
        "webpack-dev-server": "^3.2.0"
    },
    "peerDependencies": {
        "windows-build-tools": "4.0.0"
    },
    "resolutions": {
        "@types/node": "^10.1.3",
        "@types/react": "16.8.4",
        "@types/webpack": "^4.4.29"
    }
}
